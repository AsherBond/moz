<h1>Listing youtubes</h1>

<table>
  <tr>
    <th>User</th>
    <th>Title</th>
    <th>About</th>
    <th>Youtube url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @youtubes.each do |youtube| %>
  <tr>
    <td><%= youtube.user %></td>
    <td><%= youtube.title %></td>
    <td><%= youtube.about %></td>
    <td><%= youtube.youtube_url %></td>
    <td><%= link_to 'Show', youtube %></td>
    <td><%= link_to 'Edit', edit_youtube_path(youtube) %></td>
    <td><%= link_to 'Destroy', youtube, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Youtube', new_youtube_path %>
